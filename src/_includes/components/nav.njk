<nav class="side-nav zui skinny-scrollbar">
  <header>
    <a href="{{ '/' | url }}">
      <svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" viewBox="0 0 20.5 48" style="enable-background:new 0 0 20.5 48;" xml:space="preserve">
        <polygon style="fill:var(--docs-orange-color);" points="9.6,0 0,0 10.8,24 0,48 9.6,48 20.5,24 "/>
      </svg>
      Booster
    </a>
  </header>

  <section>
    <a class="nav-back-button" href="{{ (parentPermalink | url) if (parent !== system) else ('/' | url) }}">
      <svg class="svg" id="svg" viewBox="0 0 24 24">
        <path d="M15.41 7.41L14 6l-6 6 6 6 1.41-1.41L10.83 12z"></path>
      </svg>
      {{ (system | sentenceCase) if (parent !== system) else "Home" }}
    </a>

    <hr>

    <h3>{{ parent | sentenceCase }}</h3>

    <ul>
      {% for navItem in navItems %}
      {{ navItem.currentPath }}<br>
      {{ navItem.currentFile }}
        <li{% if navItem.isCurrent %} aria-current="current"{% endif %}>
          <a href="{{ navItem.url }}">{{ navItem.name | sentenceCase }}</a>
        </li>
      {% endfor %}
    </ul>
  </section>
</nav>
