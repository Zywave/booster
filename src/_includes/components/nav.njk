{% set hasChildPages = collections.all | eleventyNavigation(eleventyNavigation.key) | length > 0 %}
{% set parentObj = collections.all | eleventyNavigation(eleventyNavigation.parent) %}

{% set navCategory = eleventyNavigation.parent %}

<nav class="side-nav zui skinny-scrollbar">
  <header>
    <a href="{{ '/' | url }}"><svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" viewBox="0 0 20.5 48" style="enable-background:new 0 0 20.5 48;" xml:space="preserve"><polygon style="fill:var(--docs-orange-color);" points="9.6,0 0,0 10.8,24 0,48 9.6,48 20.5,24 "/></svg>Booster</a>
  </header>
  <section>
    <ul>
    {%- for doc in collections.designSystemAll -%}
      <li>
        <a {% if doc.url == page.url %} data-current="current item" {% endif %} href="{{ doc.url }}">{{ doc.data.title }}, parent: {{ doc.data.eleventyNavigation.parent }}, key: {{ doc.data.eleventyNavigation.key }}</a></li>
    {%- endfor -%}
    </ul>
  </section>
</nav>
