{
  "name": "@zywave/docs",
  "version": "1.0.3",
  "description": "",
  "scripts": {
    "build": "yarn run sass:build && yarn run typescript:build && eleventy",
    "serve": "eleventy --serve",
    "watch": "npm-run-all --sequential sass:build typescript:build && npm-run-all --parallel serve sass:watch typescript:watch",
    "sass:build": "sass --style=compressed src/_includes/assets/styles/styles.scss:static/css/styles.css",
    "sass:watch": "sass --watch --style=compressed src/_includes/assets/styles/styles.scss:static/css/styles.css",
    "typescript:build": "tsc -p tsconfig.json",
    "typescript:watch": "tsc -p tsconfig.json -w",
    "debug:windows": "set DEBUG=* npx eleventy",
    "debug:nix": "DEBUG=* npx eleventy"
  },
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-navigation": "^0.2.0",
    "luxon": "^1.25.0",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.3.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.34.0",
    "typescript": "^4.3.0-beta"
  },
  "dependencies": {
    "html-minifier": "^4.0.0",
    "uglify-es": "^3.3.9"
  }
}
